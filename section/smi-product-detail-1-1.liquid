  data-section="product-detail" data-name="✧Product detail 1">
   
  {% liquid
    assign inventory_variant = ''
    for variant in product.variants
    assign inventory_variant = inventory_variant | append: ', "' | append: variant.id | append: '" :{ "inventory_quantity": ' | append: variant.inventory_quantity | append: ', "inventory_policy": "' | append: variant.inventory_policy | append: '"}'
    endfor
    assign inventory_variant = inventory_variant | replace_first: ',', ''
  %}
  {% for variant in product.variants %}
    {% liquid
      assign current_variant = ""
      assign var1 = variant.option1
      assign var2 = variant.option2
      assign var3 = variant.option3
      if var1 and var1 != ""
        assign current_variant = current_variant | append: variant.option1
      endif
      if var2 and var2 != ""
        assign current_variant = current_variant | append: " _t_ " | append: var2
      endif
      if var3 and var3 != ""
      assign current_variant = current_variant | append: " _t_ " | append: var3
      endif
      assign current_variant = current_variant | replace: '"', '' | replace: "'", ""
    %}
    <input type="hidden" id="product_id_{{ forloop.index }}" value="{{ variant.id }}_${{ current_variant }}">
  {% endfor %}
  {% liquid
    assign currency_code_enabled = settings.smi_currency_code_enabled | default: shop.metafields.smind.settings.value.showCurrencyCode | default: false
      if currency_code_enabled
         assign currency_full = true
       else
        assign currency_full = false
    endif
  %}
  <div id="json_variant_{{ section.id }}"
    data-variant-check='{{ product.variants | json | escape }}'
    data-variant-inventory-quatity='{{ inventory_variant }}'
