{% liquid
assign currency_code_enabled = settings.smi_currency_code_enabled | default: shop.metafields.smind.settings.value.showCurrencyCode | default: false
  assign target = product.selected_or_first_available_variant
  assign selectedVariantId = target.id
  assign compare_at_price = target.compare_at_price
  assign price = target.price | default: 1999
  assign selectedVariantTitle = target.title
  assign available = target.available | default: false
  assign money_price = price | money
  if currency_code_enabled
    assign money_price = price | money_with_currency
  endif
  if target == product and product.price_varies
    assign money_price = 'products.product.price.from_price_html' | t: price: money_price
  endif
  assign show_buy_quantity_btn = false
  assign has_buy_group = false
  assign has_color_block = false
  for block in section.blocks
    if block.type == 'product_quantity'
      if block.settings.display_in_buy_button_block == true
        assign show_buy_quantity_btn = true
      else
        assign show_buy_quantity_btn = false
      endif
    endif
    if block.type == 'product_purchase'
      assign has_buy_group = true
    endif
    if block.type == 'product_color'
      assign has_color_block = true
    endif
    if block.type == 'product_size_chart'
    assign size_chart_append_to_variant_block = block.settings.append_to_variant_block
    assign show_icon_size_chart =  block.settings.show_icon
    endif
  endfor
  if section.settings.desktop_media_position == 'right'
    assign desktop_media_position = 'smi-flex-md-row-reverse'
  endif
  comment
    Check to any block displayed below the gallery
  endcomment
  assign padding_bottom_gallery = false
  for block in section.blocks
    if 'product_description product_collapse_content product_ask_questions' contains block.type and block.settings.show_desc_below == true
      assign padding_bottom_gallery = true
    endif
  endfor
%}
<div
  id="smi-{{ section.id }}"
