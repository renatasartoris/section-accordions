{%- liquid
  assign media_height = section.settings.media_height
  assign media_ratio = section.settings.media_ratio
  assign collapse_index = 0
  assign media_shape = section.settings.media_shape
  assign collapsible_content_type = 4
  assign author_classes = ''
  if media_shape == 'default'
    if section.settings.full_width == 'container-fluid-no-padding'
      assign author_classes = 'smi-media-wrapper smi-media-settings'
    else
      assign author_classes = 'smi-media-wrapper smi-media-settings smi-border smi-shadow '
    endif
    if media_height == 'x-small'
      assign author_classes = author_classes | append: ' smi-media-height-x-small'
    elsif media_height == 'small'
      assign author_classes = author_classes | append: ' smi-media-height-small'
    elsif media_height == 'medium'
      assign author_classes = author_classes | append: ' smi-media-height-medium'
    elsif media_height == 'large'
      assign author_classes = author_classes | append: ' smi-media-height-large'
    endif
  else
    assign author_classes = 'smi-media-wrapper smi-lh-0 smi-overflow-hidden smi-p-relative ' | append: media_shape
  endif
  if media_ratio == 'auto'
    assign author_classes = author_classes | append: ' smi-h-auto'
  else
    assign author_classes = author_classes | append: ' smi-h-100'
  endif
  assign content_position = section.settings.content_position
  assign collapsible_type = section.settings.collapsible_type
  assign color_pos = 0
  assign bkg_pos = 0
  for block in section.blocks
  if block.type == 'color'
  assign color_pos = forloop.index
  endif
  if block.type == 'background'
  assign bkg_pos = forloop.index
  endif
endfor
